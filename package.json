{
  "name": "PocketViewer",
  "version": "1.0.0",
  "description": "",
  "author": "Shota Takeshima",
  "private": true,
  "scripts": {
    "dev": "npm run cp_env_dev && cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:nuxt": "npm run cp_env_dev && cross-env NODE_ENV=development ./node_modules/nuxt/bin/nuxt start",
    "build": "nuxt build",
    "start": "npm run cp_env_prod && cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "cp_env_dev": "cp .env_dev .env",
    "cp_env_prod": "cp .env_prod .env"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@mdi/font": "^3.4.93",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.0.0",
    "nuxt-user-agent": "^1.0.2",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "aws-sdk": "^2.814.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "engines": {
    "node": "~8.x.x"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ]
}
